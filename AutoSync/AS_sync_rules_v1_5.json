{
  "version": "1.5",
  "scan_interval_seconds": 0,
  "watch_paths": [
    "../OmegaEngine",
    "../Scheduler",
    "../SafeGuard",
    "../AutoSync"
  ],
  "include_extensions": [
    ".py",
    ".json",
    ".txt"
  ],
  "exclude_dirs": [
    "__pycache__",
    ".git",
    ".idea"
  ],
  "exclude_files": [
    "AS_state_v1_4.json",
    "AS_report_v1_4.json",
    "AS_state_v1_5.json",
    "AS_report_v1_5.json"
  ],
  "unused_file_patterns": [
    "__pycache__",
    ".pyc",
    "requirements.txt"
  ],
  "modules": [
    {
      "name": "OmegaEngine",
      "root": "../OmegaEngine",
      "critical": true
    },
    {
      "name": "Scheduler",
      "root": "../Scheduler",
      "critical": true
    },
    {
      "name": "SafeGuard",
      "root": "../SafeGuard",
      "critical": true
    },
    {
      "name": "AutoSync",
      "root": "../AutoSync",
      "critical": true
    }
  ]
}
